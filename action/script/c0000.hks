                if arts_hand == HAND_RIGHT then
                    return ATTACK_REQUEST_ARROW_BOTH_RIGHT
                else
                    return ATTACK_REQUEST_ARROW_BOTH_LEFT
                end
            else
                return swordartrequest
            end
        elseif HAND_LEFT_BOTH <= style then
            return ATTACK_REQUEST_BOTH_HEAVY
