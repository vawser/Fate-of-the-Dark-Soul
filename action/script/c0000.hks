        if 50 < rand then
            r2 = "W_RandomOneShotNext1"
            b2 = "W_RandomOneShotNext1"
        else
            r2 = "W_RandomOneShotNext3"
            b2 = "W_RandomOneShotNext3"
        end
    elseif ignorenext4 == TRUE then
        if 50 < rand then
            r2 = "W_RandomOneShotNext1"
            b2 = "W_RandomOneShotNext1"
        else
            r2 = "W_RandomOneShotNext3"
            b2 = "W_RandomOneShotNext3"
        end
