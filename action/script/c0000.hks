        return 
    elseif env(IsAnimationOver, 3) == TRUE then
        if env(IsEquipmentMenuOpen) == TRUE then
            ExecEventNoReset("W_InGameMenuIdleStart")
        else
            ExecEventNoReset("W_Idle")
        end
        return 
    else
        return 
    end
end

