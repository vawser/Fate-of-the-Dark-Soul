function AddToSet(set, key)
    set[key] = true
end

function RemoveFromSet(set, key)
    set[key] = nil
end

function SetContains(set, key)
    return set[key] ~= nil
end

-- Checks the spAtkCategory for the primary hand
function IsSpecialCategory(name)
    local hand = HAND_RIGHT
    
    if style == HAND_LEFT_BOTH then
        hand = HAND_LEFT
    end
    
    local sp_kind = env(GetEquipWeaponSpecialCategoryNumber, hand)
    
    if SetContains(special_category_table, name) then
        if sp_kind == special_category_table[name] then
            return true
        else
            return false
        end
    else
        return false
    end
end

