        elseif env(GetEventEzStateFlag, 0) == TRUE and env(ActionDuration, ACTION_ARM_L2) <= 0 then
            ExecEventHalfBlend(Event_ChargeShotRightEnd, ALLBODY)
            return 
        elseif env(IsAnimationOver, 1) == TRUE then
